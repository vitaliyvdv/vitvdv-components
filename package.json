{
  "name": "vitvdv-components",
  "version": "0.1.8",
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitaliyvdv/vitvdv-components.git"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "dependencies": {
    "@loadable/component": "^5.15.0",
    "framer-motion": "^4.1.17",
    "lodash": "^4.17.21",
    "react-inlinesvg": "^2.3.0",
    "react-intersection-observer": "^8.32.1",
    "react-tooltip-lite": "^1.12.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.4",
    "@babel/core": "^7.15.5",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "np": "^7.5.0",
    "onchange": "^7.1.0",
    "path": "^0.12.7",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "storybook": "^6.3.7",
    "styled-components": "^5.3.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-prettier": "^1.2.0",
    "stylelint-processor-styled-components": "^1.10.0",
    "tailwindcss": "^2.2.14",
    "twin.macro": "^2.7.0"
  },
  "peerDependencies": {
    "babel-plugin-macros": "^3.1.0",
    "babel-plugin-styled-components": "^1.13.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1",
    "tailwindcss": "^2.2.14",
    "twin.macro": "^2.7.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "rm -rf dist && NODE_ENV=production babel src/components --out-dir dist --copy-files --ignore __tests__,spec.js,test.js,__snapshots__",
    "test": "echo \"No test specified\"",
    "eject": "react-scripts eject",
    "prettier": "prettier --config .prettierrc --config-precedence prefer-file --write './**/*.{js,jsx,css,scss,json}'",
    "prettier-watch": "onchange -d 1000 './**/*.{js,jsx,css,scss,json}' -- prettier --config .prettierrc --config-precedence prefer-file --write {{changed}}",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "np-run": "np --no-tests --no-2fa",
    "npm-publish": "yarn build && yarn np-run"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "babelMacros": {
    "twin": {
      "preset": "styled-components",
      "config": "./tailwind.config.js",
      "includeClassNames": true,
      "disableColorVariables": false
    }
  }
}
